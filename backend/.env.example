# FarBrain Environment Configuration

# OpenAI Settings
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4

# Embedding Configuration
EMBEDDING_MODEL=sentence-transformers/paraphrase-multilingual-mpnet-base-v2
EMBEDDING_DIMENSION=768

# Database Configuration
DATABASE_URL=sqlite+aiosqlite:///./farbrain.db

# CORS Settings
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:5174

# Security
SECRET_KEY=dev-secret-key-change-in-production
ADMIN_PASSWORD=admin123

# Clustering Parameters
MIN_IDEAS_FOR_CLUSTERING=10
CLUSTERING_INTERVAL=10
CLUSTER_SAMPLE_SIZE=10

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
